      "path": "https://creativecommons.org/licenses/by/4.0/"
    }
  ],
  "resources": [
    {
      "path": "noleggio_magnetoterapia.csv",
      "name": "noleggio_magnetoterapia",
      "format": "csv",
      "schema": {
        "fields": [
          {"name": "id", "type": "integer"},
          {"name": "nome_macchinario", "type": "string"},
          {"name": "descrizione", "type": "string"},
          {"name": "durata_giorni", "type": "integer"},
          {"name": "prezzo_base", "type": "number"},
          {"name": "disponibilità", "type": "string"},
          {"name": "sito_web", "type": "string"},
